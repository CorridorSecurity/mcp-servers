{
  "name": "PostHog",
  "description": "Analytics, error tracking, and feature flags.",
  "transport": [
    "sse"
  ],
  "icon": "./icon.svg",
  "oauth": false,
  "config": {
    "command": "npx",
    "args": [
      "-y",
      "mcp-remote@latest",
      "https://mcp.posthog.com/sse",
      "--header",
      "Authorization:${POSTHOG_AUTH_HEADER}"
    ],
    "env": {
      "POSTHOG_AUTH_HEADER": "Bearer {INSERT_YOUR_PERSONAL_API_KEY_HERE}"
    }
  }
}