{
  "name": "Select Star",
  "description": "Data catalog, lineage, and context.",
  "transport": [
    "stdio"
  ],
  "icon": "./icon.svg",
  "oauth": false,
  "config": {
    "command": "npx",
    "args": [
      "-y",
      "mcp-remote@latest",
      "https://mcp.production.selectstar.com/mcp",
      "--header",
      "Authorization:${SELECT_STAR_TOKEN}"
    ],
    "env": {
      "SELECT_STAR_TOKEN": ""
    }
  }
}