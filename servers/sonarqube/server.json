{
  "name": "SonarQube",
  "description": "Analyze code with SonarQube.",
  "transport": [
    "stdio"
  ],
  "icon": "./icon.svg",
  "oauth": false,
  "config": {
    "command": "docker",
    "args": [
      "run",
      "-i",
      "--rm",
      "-e",
      "SONARQUBE_TOKEN",
      "-e",
      "SONARQUBE_URL",
      "mcp/sonarqube"
    ],
    "env": {
      "SONARQUBE_TOKEN": "",
      "SONARQUBE_URL": ""
    }
  }
}